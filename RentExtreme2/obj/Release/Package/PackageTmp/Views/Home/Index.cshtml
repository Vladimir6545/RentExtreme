@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1></h1>
    <div id="myCarousel" class="carousel slide">
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
            <li data-target="#myCarousel" data-slide-to="3"></li>
        </ol>
        <div class="carousel-inner">
            <div class="item active">
                <a href="@Url.Action("Index","Coordinates", new { id=1})">
                    <img src="~/Content/Images/snouboard.jpg" alt="">

                    <div class="carousel-caption">
                        <h4></h4>
                    </div>
                </a>
            </div>
            <div class="item">
                <a href="@Url.Action("Index","Coordinates", new { id=2})">
                    <img src="~/Content/Images/bicycle.jpg" alt="">

                    <div class="carousel-caption">
                    </div>
                </a>
            </div>
            <div class="item">
                <a href="@Url.Action("Index","Coordinates", new { id=3})">
                    <img src="~/Content/Images/skiing.jpg" alt="">

                    <div class="carousel-caption">
                    </div>
                </a>
            </div>
            <div class="item">
                <a href="@Url.Action("Index","Coordinates", new { id=4})">
                    <img src="~/Content/Images/rollers.jpg" alt="">
                    <div class="carousel-caption">
                    </div>
                </a>
            </div>
        </div>
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">&lsaquo;</a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">&rsaquo;</a>
    </div>
</div>